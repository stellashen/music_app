<h1>Bands:</h1>

<ul>
  <% @bands.each do |band| %>
    <li><a href="<%= band_url(band) %>"><%= band.name %></a></li>
  <% end %>
</ul>

<p> Don't see the band you love? Click the button below:</p>
<form action="<%= new_band_url %>" method="get">
  <%= auth_token %>
  <input type="submit" value="Add a band">
</form>

<% content_for :footer do %>
  This is cats index.
<% end %>
